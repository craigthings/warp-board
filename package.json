{
  "name": "warp-board",
  "version": "0.1.0",
  "type": "module",
  "description": "A spatial, hierarchical documentation system with markdown boards",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite --mode electron"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "marked": "^17.0.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "slugify": "^1.6.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "typescript": "^5.3.3",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "appId": "com.warpboard.app",
    "productName": "Warp Board",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
